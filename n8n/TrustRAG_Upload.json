    {
      "id": "up-extract-text",
      "name": "Extract and Chunk Text",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        1340,
        100
      ],
      "parameters": {
        "jsCode": "const req = $('Extract Request').first().json;\nconst perm = $('Check Upload Permission').first().json;\n\n// base64 디코드\nconst text = Buffer.from(req.filedata, 'base64').toString('utf8');\n\n// 단락 기준으로 청크 분할 (최대 1000자)\nconst paragraphs = text.split(/\\n\\n+/).filter(p => p.trim().length > 0);\nconst chunks = [];\nlet current = '';\n\nfor (const para of paragraphs) {\n  if ((current + '\\n\\n' + para).length > 800) {\n    if (current) chunks.push(current.trim());\n    current = para;\n  } else {\n    current = current ? current + '\\n\\n' + para : para;\n  }\n}\nif (current.trim()) chunks.push(current.trim());\n\nconst metadata = {\n  source_filename: req.filename,\n  filesize: req.filesize,\n  mimetype: req.mimetype,\n  category: req.category,\n  uploaded_at: new Date().toISOString()\n};\n\nreturn chunks.map((chunk, i) => ({\n  json: {\n    chunk,\n    chunk_index: i,\n    total_chunks: chunks.length,\n    metadata,\n    company_id: perm.company_id,\n    table_name: perm.selected_category.table_name,\n    filename: req.filename\n  }\n}));"
      }
    },
