    {
      "parameters": {
        "jsCode": "const auth = $('Validate Auth').first().json;\nconst requested = $('Extract Request').first().json.category;\nconst cats = auth.categories || [];\nconst UPLOAD_ROLES = ['super_admin','company_admin','group_admin'];\n\nif (!UPLOAD_ROLES.includes(auth.role)) {\n  return [{ json: { ...auth, error: 'Upload requires group_admin role or higher' } }];\n}\n\nconst cat = cats.find(c => c.name === requested || (c.table_name||'').includes(requested) || (c.table_name||'') === requested);\n\nif (!cat) {\n  return [{ json: { ...auth, error: 'Category not found or access denied' } }];\n}\n\nif (cat.can_upload === false) {\n  return [{ json: { ...auth, error: 'Upload permission denied for this category' } }];\n}\n\nreturn [{ json: { ...auth, selected_category: cat } }];"
      },
